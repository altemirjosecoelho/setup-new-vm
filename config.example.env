# Configurações de VM
VM_HOST=192.168.1.100
VM_USER=jenkins
SSH_KEY_PATH=/var/lib/jenkins/.ssh/id_rsa

# Versões das ferramentas
NODE_VERSION=18.x
DOCKER_VERSION=latest
POSTGRES_VERSION=15
REDIS_VERSION=7

# Configurações de containers
POSTGRES_CONTAINER=postgres-app
REDIS_CONTAINER=redis-app
POSTGRES_PORT=5432
REDIS_PORT=6379
POSTGRES_PASSWORD=postgres123
POSTGRES_DB=appdb

# Configurações de Jenkins
JENKINS_URL=https://jenkins.controller.possoatender.com/
PROJECT_NAME=setup-new-vm
GITHUB_REPO=https://github.com/seu-usuario/setup-new-vm

# Configurações de aprovação
APPROVAL_REQUIRED=true
APPROVAL_TIMEOUT=30
APPROVED_USERS=admin,devops,team-lead
AUTO_DEPLOY_BRANCHES=hotfix,release
